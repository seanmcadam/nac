#!/usr/bin/perl


use Data::Dumper;
use FindBin;
use lib "$FindBin::Bin/../lib";
use NAC::LocalLogger;
use NAC::Worker::Thread;
use NAC::Worker::LocalConfig;
use strict;

# use constant LOCAL_DB_THREAD_COUNT => 10;
use constant LOCAL_DB_THREAD_COUNT => 1;

my $thread = NAC::Worker::Thread->new();
for my $i ( 1 .. LOCAL_DB_THREAD_COUNT ) {
  $thread->create( \&NAC::Worker::LocalConfig::new );
  #$LOGGER_DEBUG_1->( EVENT_START, " Local Config Thread Started " );
}
$thread->run();



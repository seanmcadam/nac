#!/usr/bin/perl


use Data::Dumper;
use FindBin;
use lib "$FindBin::Bin/../lib";
use NAC::Worker::Thread;
use NAC::Worker::LocalDB;
use strict;

use constant LOCAL_DB_THREAD_COUNT => 10;

my $thread = NAC::Worker::Thread->new();
for my $i ( 1 .. LOCAL_DB_THREAD_COUNT ) {
  $thread->create( \&NAC::Worker::LocalDB::new );
}
$thread->run();


